<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="breeze.js">
            <arguments>
                <argument name="bundles" xsi:type="array">
                    <item name="default" xsi:type="array">
                        <item name="items" xsi:type="array">
                            <item name="extendWarrantyOffers" xsi:type="string">
                                Swissup_BreezeExtendPurchasePriceOffers/js/warranty-offers-base
                            </item>
                            <item name="Extend_Warranty/js/view/minicart-mixin" xsi:type="string">
                                Swissup_BreezeExtendPurchasePriceOffers/js/view/minicart-mixin
                            </item>
                        </item>
                    </item>
                    <item name="product" xsi:type="array">
                        <item name="items" xsi:type="array">

                            <item name="simpleProductWarranty" xsi:type="string">
                                Swissup_BreezeExtendPurchasePriceOffers/js/product/simple-warranty
                            </item>

                        </item>
                    </item>
                </argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
